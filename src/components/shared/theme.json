{
  "collectionName": "components_shared_themes",
  "info": {
    "displayName": "Theme"
  },
  "options": {},
  "attributes": {
    "theme": {
      "type": "enumeration",
      "default": "light",
      "enum": [
        "semi_light",
        "light",
        "dark",
        "bgImage"
      ]
    },
    "bgImage": {
      "type": "media",
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "theme"
            },
            "bgImage"
          ]
        }
      },
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    }
  },
  "config": {}
}
